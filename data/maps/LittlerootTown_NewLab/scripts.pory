script LittlerootTown_NewLab_EventScript_Pawniard {
    showmonpic(SPECIES_PAWNIARD, 10,3)
    msgbox("Do you want to choose Pawniard?", MSGBOX_YESNO)
    if(var(VAR_RESULT) == YES){
        givemon(SPECIES_PAWNIARD, 5, isShiny=YES)
        if(var(VAR_RESULT) == MON_GIVEN_TO_PARTY){
            playfanfare(MUS_OBTAIN_ITEM)
            msgbox("You received Pawniard!")
            removeobject(1)
            setflag(FLAG_UNUSED_0x493)
            setflag(FLAG_SYS_POKEMON_GET)
            call(NicknamePartyMon)
        }
        elif(var(VAR_RESULT) == MON_GIVEN_TO_PC){
            playfanfare(MUS_OBTAIN_ITEM)
            msgbox("You received Pawniard!")
            removeobject(1)
            setflag(FLAG_UNUSED_0x493)
            setflag(FLAG_SYS_POKEMON_GET)
            call(NicknameBoxMon)
        }
        else{
            hidemonpic
            msgbox("No more room")
        }
    }
    else{
        hidemonpic
        msgbox(NewLab_NotChosen)
    }

}

