mapscripts LittlerootTown_NewLab_MapScripts {
    MAP_SCRIPT_ON_TRANSITION: LittlerootTown_NewLab_EventScript_BallDefault
}

script LittlerootTown_NewLab_EventScript_BallDefault{
    setvar(VAR_UNUSED_0x40F7, 0)
}

script LittlerootTown_NewLab_EventScript_Birch{
    lockall
    faceplayer
    if(!(flag(FLAG_UNUSED_0x493) || flag(FLAG_UNUSED_0x494) || flag(FLAG_UNUSED_0x495))){
        msgbox("Hello, would you like to see more options?", MSGBOX_YESNO)
        if(var(VAR_RESULT) == YES){
            if(var(VAR_UNUSED_0x40F7) != 2){
                addvar(VAR_UNUSED_0x40F7, 1)
            }
            else{
                setvar(VAR_UNUSED_0x40F7, 0)
            }
            msgbox("Choose your starter wisely.")
        }
        else{
            msgbox("Choose your starter wisely.")
        }
    }
    else{
        msgbox("Take care of your Pokemon!")
    }
    waitmessage
    releaseall
    end
}

script LittlerootTown_NewLab_EventScript_PokeballOne{
    lockall
    if(!(flag(FLAG_UNUSED_0x493) || flag(FLAG_UNUSED_0x494) || flag(FLAG_UNUSED_0x495))){
        if(var(VAR_UNUSED_0x40F7) == 0){
            call(LittlerootTown_NewLab_EventScript_Pawniard)
        }
        elif(var(VAR_UNUSED_0x40F7) == 1){
            call(LittlerootTown_NewLab_EventScript_Litwick)
        }
        elif(var(VAR_UNUSED_0x40F7) == 2){
            call(LittlerootTown_NewLab_EventScript_Tynamo)
        }
    }
    else{
        msgbox("Don't take another!")
    }
    waitmessage
    releaseall
    end
}

